{
  "version": 2,
  "framework": null,
"builds": [
   { "src": "public/index.php",
       "use":"vercel-php@0.7.4" }
  ],
  "routes": [
      
        {
            "src": "/(.*\\.(?:css|js|png|jpg|jpeg|gif|svg|ico|ttf|woff|woff2|eot|otf|webp|avif|txt))$",
            "dest": "/public/$1"
        },
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
      
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
    
    "outputDirectory": "public"
}
